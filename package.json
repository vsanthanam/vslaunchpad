{
  "name": "vslaunchpad",
  "version": "0.1.0",
  "homepage": "https://www.talktovarun.com",
  "private": true,
  "dependencies": {
    "ajv": "^6.4.0",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "npm-run-all": "^4.1.2",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-modal": "^3.3.2",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.1.4",
    "webpack": "^4.5.0"
  },
  "scripts": {
    "build-css": "less-watch-compiler --run-once --main-file index.less src src",
    "watch-css": "yarn run build-css && less-watch-compiler --main-file index.less src src",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all -p build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "flow": "flow",
    "deploy": "firebase deploy"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-flow": "^6.23.0",
    "flow-bin": "^0.69.0",
    "less-watch-compiler": "^1.11.2"
  }
}
